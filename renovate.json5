{
  $schema: "https://docs.renovatebot.com/renovate-schema.json",
  baseBranches: [
    "develop",
  ],
  extends: [
    "config:base",
  ],
  labels: [
    "dependencies",
  ],
  timezone: "Asia/Tokyo",
  prConcurrentLimit: 10,
  packageRules: [
    // kotlin version: jetpack composeが非対応のため
    {
      groupName: "kotlin version",
      matchPackagePatterns: [
        "org.jetbrains.kotlin:"
      ],
      labels: [
        "kotlin-version"
      ],
      matchCurrentVersion: ">=1.7.20"
    }
  ]
}